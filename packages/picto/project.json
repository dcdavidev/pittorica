{
  "name": "picto",
  "root": "packages/picto",
  "sourceRoot": "packages/picto/src",
  "projectType": "library",
  "tags": ["scope:picto", "type:ui"],
  "targets": {
    "build": {
      "dependsOn": ["doc:g:toc", "^build"]
    },
    "doc:g:toc": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "pnpm dlx markdown-toc -i README.md"
      }
    }
  }
}
