{
  "name": "picto",
  "root": "packages/picto",
  "sourceRoot": "packages/picto/src",
  "projectType": "library",
  "tags": ["scope:react"],
  "implicitDependencies": [],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm dlx eslint --no-warn-ignored --fix ./packages/picto"]
      }
    },
    "fmt": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm exec prettier --ignore-unknown --write ./packages/picto"
        ]
      }
    },
    "lint:fmt": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["pnpm dlx nx run picto:lint", "pnpm dlx nx run picto:fmt"]
      }
    },
    "doc:g:toc": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["pnpm dlx markdown-toc -i README.md"]
      }
    },
    "doc:g": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run-many -t doc:g:toc -p picto"]
      }
    }
  }
}
