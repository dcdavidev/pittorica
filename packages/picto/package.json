{
  "name": "@pittorica/picto",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/dcdavidev/pittorica#readme",
  "bugs": "https://github.com/dcdavidev/pittorica/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcdavidev/pittorica.git",
    "directory": "packages/picto"
  },
  "author": "Davide Di Criscito",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@pittorica/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components": {
      "@pittorica/source": "./src/components/index.ts",
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./css": {
      "sass": "./src/index.scss",
      "style": "./dist/index.css",
      "import": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./types": {
      "@pittorica/source": "./src/types/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "dependencies": {
    "clsx": "^2.1.1",
    "html-react-parser": "^5.2.10"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "postcss-custom-properties": "^14.0.6",
    "postcss-load-config": "^6.0.1",
    "postcss-nested": "^7.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sass": "./src/index.scss"
}
